<% provide(:title, "Signup") %>
<div id= "signup_message">
  <h1>Fill in the information below to sign up:</h1>
</div>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_with(model: @user, local: true) do |f| %>
      <%= f.label :first_name %>
      <%= f.text_field :first_name %>

      <%= f.label :last_name %>
      <%= f.text_field :last_name %>

      <%= f.label :email %>
      <%= f.text_field :email %>

      <%= f.label :password %>
      <%= f.text_field :password %>
      <%= f.submit "Finish", class: "btn btb-primary" %>
  <% end %>
  </div>
</div>